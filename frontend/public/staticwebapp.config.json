{
  "routes": [
    {
      "route": "/static/*",
      "rewrite": "/static/"
    },
    {
      "route": "assets/*",
      "rewrite": "/assets/"
    },
    {
      "route": "/favicon.ico",
      "rewrite": "/favicon.ico"
    },
    {
      "route": "/robots.txt",
      "rewrite": "/robots.txt"
    },
    {
      "route": "/*",
      "rewrite": "/index.html"
    }
  ]
}
