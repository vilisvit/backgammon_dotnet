{
  "routes": [
    {
      "route": "/static/*",
      "rewrite": "/static/{path}"
    },
    {
      "route": "/assets/*",
      "rewrite": "/assets/{path}"
    },
    {
      "route": "/favicon.ico",
      "rewrite": "/favicon.ico"
    },
    {
      "route": "/robots.txt",
      "rewrite": "/robots.txt"
    },
    {
      "route": "/*",
      "rewrite": "/index.html"
    }
  ]
}
