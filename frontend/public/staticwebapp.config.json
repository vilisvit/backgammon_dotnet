{
  "routes": [
    {
      "route": "/static/*",
      "serve": "/static/"
    },
    {
      "route": "/assets/*",
      "serve": "/assets/"
    },
    {
      "route": "/favicon.ico",
      "serve": "/favicon.ico"
    },
    {
      "route": "/robots.txt",
      "serve": "/robots.txt"
    },
    {
      "route": "/*",
      "rewrite": "/index.html"
    }
  ]
}
